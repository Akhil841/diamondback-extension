(fun (bin-search v x)
    (let ((lo 0)
          (hi (vec-len v)))
        (loop
            (if (< lo hi)
                (let ((mid (+ lo (/ (- hi lo) 2))))
                    (if (<= x (vec-get v mid))
                        (set! hi mid)
                        (set! lo (+ mid 1))))
                (break lo)))))

